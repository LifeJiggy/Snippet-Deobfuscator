  },
  "detection": {
    "patterns": {
      "base64": {
        "regex": "^[A-Za-z0-9+/]+={0,2}$",
        "minLength": 4,
        "lengthModulo": 4,
        "checkUTF8": true
      },
      "hex": {
        "regex": "^[0-9a-fA-F]+$",
        "minLength": 2,
        "lengthModulo": 2
      },
      "unicode": {
        "regex": "\\\\u[0-9a-fA-F]{4}",
        "minOccurrences": 1
      },
      "url": {
        "regex": "%[0-9A-Fa-f]{2}",
        "minOccurrences": 1
      },
      "html": {
        "regex": "&[a-zA-Z]+;|&#\\d+;|&#x[0-9a-fA-F]+;",
        "minOccurrences": 1
      },
      "rot13": {
        "checkChars": "a-zA-Z",
        "preserveCase": true
      },
      "xor": {
        "keyDetection": true,
        "commonKeys": ["0x00", "0x41", "0x55", "0xAA", "0xFF"],
        "bruteForce": false
      }
    },
    "validation": {
      "printableThreshold": 0.5,
      "utf8Validation": true,
      "minLength": 1,
      "maxLength": 1000000,
      "syntaxCheck": true,
      "contextAware": true
    },
    "heuristics": {
      "tryAllMethods": true,
      "prioritizePatterns": true,
      "confidenceThreshold": 0.7,
      "maxAttempts": 10,
      "fallbackEnabled": true
    }
  },
  "configuration": {
    "cache": {
      "enabled": true,
      "maxSize": 1000,
      "ttl": 3600000
    },
    "performance": {
      "timeout": 5000,
      "maxStringSize": 10000000,
      "parallelDecoding": true,
      "earlyExit": true
    },
    "output": {
      "includeMethod": true,
      "includeConfidence": true,
      "includeAttempts": true,
      "includeWarnings": true,
      "includeStatistics": true
    },
    "logging": {
      "level": "info",
      "verboseErrors": false,
      "trackPerformance": true
    }
  },
  "dependencies": {
    "required": ["crypto"],
    "optional": [],
    "version": "Node.js >= 14.0.0"
  },
  "entry": "string-decryption.js",
  "priority": 1,
  "timeout": 5000,
  "retries": 3,
  "examples": [
    {
      "input": "SGVsbG8gV29ybGQ=",
